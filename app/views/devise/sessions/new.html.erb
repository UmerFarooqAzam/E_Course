<div class="row">
  <div class="col m6 offset-m3 center">
    <h2>Log in</h2>
  
    <%= form_for(resource, as: resource_name,  url: session_path(resource_name)) do |f| %>
             <%= render partial: 'shared/devisemes'%>
      <div class="input-field">
         <%= f.email_field :email,id:"email",class:"validate",placeholder:"Enter Email"  %>
      </div>

      <div class="input-field">
        <%= f.password_field :password, autocomplete: "off",class:"validate" ,placeholder:"Enter Password"%>
      </div>

      <button class="btn waves-effect waves-light" type="submit" name="action">Login</button>
      <%= link_to "Forgot Password" , new_user_password_path ,class:"btn waves-effect waves-light" %>
    <% end %>
        
  </div>
  
</div>
